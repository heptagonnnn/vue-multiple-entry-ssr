	"scripts": {
		"dev": "rm -rf ./dist && node build/dev.js",
		"dev-server": "rm -rf ./dist && NODE_ENV=development nodemon build/dev.js server ",
		"build": "rm -rf ./dist && concurrently \"npm run build:client-bundle\" \"npm run build:server-bundle\" \"npm run build:server\"",
		"build:server-bundle": "NODE_ENV=production node build/build.js server",
		"build:client-bundle": "NODE_ENV=production node build/build.js client",
		"build:server": "./node_modules/.bin/ncc build build/server.js && mv dist/index.js dist/server.js && cp ./cluster.json ./dist"
	},
